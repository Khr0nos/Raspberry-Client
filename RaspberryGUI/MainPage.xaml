<Page
    x:Class="RaspberryGUI.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:RaspberryGUI"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <TextBox x:Name="output" HorizontalAlignment="Center" Margin="409,104,1120,810" TextWrapping="Wrap" Text="" VerticalAlignment="Center" IsReadOnly="True" FontSize="26"/>
        <Ellipse x:Name="status" Fill="#FFF4F4F5" Height="70" Margin="996,104,0,0" Stroke="Black" VerticalAlignment="Top" Width="65" HorizontalAlignment="Left" IsDoubleTapEnabled="False" IsRightTapEnabled="False" IsHoldingEnabled="False" IsTapEnabled="False"/>
    </Grid>
</Page>
